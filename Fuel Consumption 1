#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      float lit,dis;
      float lit_per_100km,miles,gal,miles_gal;
      scanf("%f",&lit);
     scanf("%f",&dis);
     if(lit<=0||dis<=0)
        { 
         printf("Invalid Input");
         }
    else
        {
         lit_per_100km=(lit/dis)*100;
         miles=dis*0.6214;
         gal=lit*0.2642;
         miles_gal=miles/ gal;
        printf("Liters/100KM\n%.2f\n",lit_per_100km);
        printf("Miles/gallons\n%.2f",miles_gal);
       } 
    
    return 0;
}
