#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int Square(int n) 
{
   return n*n;
}

int main() 
{
    int n;
    scanf("%d", &n);
    if (n>=1&&n<=9) 
    {
     int res =Square(n);
     printf("Square of %d is %d\n", n, res);
    } 
    else 
    {
      printf("Invalid Input\n");
    }    
    return 0;
}
