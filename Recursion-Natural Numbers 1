#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void Natural(int n, int current) 
{
   if (current > n) return;
   printf("%d ", current);
   Natural(n, current + 1);
}
int main() 
{
     int n;
     scanf("%d", &n);
     if (n >= 1 && n <= 50) 
     {
      printf("The first %d Natural Numbers are:\n", n);
      Natural(n, 1);
     } 
    else 
    {
      printf("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!\n");
    }
  return 0;
}
