#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int val,sum=0;
    scanf("%d",&val);
    int arr[val];
    for(int i=0;i<val;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
    int mean=(float)sum/val;
    printf("Array Mean Value is %d.00",mean);
    return 0;
}
